# Signal Proxy - Production Environment
# Copy this file to .env and customize with your production values

# Environment: development or production
APP_ENV=production

# Domain settings - replace with your actual domain
DOMAIN=proxy.yourdomain.com
BASE_URL=https://proxy.yourdomain.com

# Debug mode (should be false in production)
DEBUG=false

# Log level: debug, info, warn, error
LOG_LEVEL=info
